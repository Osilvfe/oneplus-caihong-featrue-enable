    <!-- sean.liu@ANDROID.DisplayFramework, 2022/06/13, add for oplus embedding -->
    <oplus-feature name="oplus.software.display.google_extension_layout" />

    <!-- sean.liu@ANDROID.DisplayFramework, 2022/05/09, add for oplus embedding -->
    <oplus-feature name="oplus.software.display.oplus_activity_embedding" />

    <!-- wanchang, 2023/10/14, Add for keep alive autotest -->
